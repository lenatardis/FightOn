{
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "svgo": "^4.0.0"
  },
  "scripts": {
    "svgo:clean-opt-dir": "rm -rf assets-opt && mkdir assets-opt",
    "svgo:copy": "rsync -a --include='*/' --include='*.svg' --exclude='*' assets/ assets-opt/",
    "svgo:opt": "svgo -f assets-opt --recursive --config=svgo.config.cjs --quiet",
    "svgo:report": "node tools/svgo-report.cjs",
    "svgo:safe": "npm-run-all svgo:clean-opt-dir svgo:copy svgo:opt svgo:report"
  }
}
